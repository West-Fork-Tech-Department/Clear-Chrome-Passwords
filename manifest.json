{
    "manifest_version": 3,

    "name": "Clear Chrome Passwords",
    "description": "Removes all passwords from the users Chrome Password Manager.",
    "version": "1.0",

    "icons": {
        "48": "password.png"

    },
    "background": {
        "service_worker": "background.js"
      },  
    "permissions": [
        "browsingData"
    ]

}
